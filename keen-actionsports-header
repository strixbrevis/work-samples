<script type="text/javascript">
	function bcAd() {
		var a = document.createElement('span');
			a.setAttribute('id', 'crumbsponsor');
//		var b = document.createElement('img');
//			b.setAttribute('src', 'http://ad.doubleclick.net/ad/N2958.128037.ACTIVE/B5091938.4;sz=1x1');
		var d = $('breadcrumbs');
		d.appendChild(a);
//		d.appendChild(b);
		$('crumbsponsor').update('Presented by <a href="http://www.keenfootwear.com/?TAN=38-24" target="_blank""><img src="/Assets/sponsors/keen/keen-breadcrumb-logo.gif" alt="Keen" id="bcsponimage"/></a>');
	}
	document.observe("dom:loaded", function() {
		var d = $('breadcrumbs');
		if (d) {
			bcAd();
		} else {
			bcAd.delay(0.1);
		}
	});
 	var skin_wrapper = document.createElement("div");
 			skin_wrapper.setAttribute("id", "skin_wrapper");

 	 var skin_wrapper_content = document.createElement("a");
			skin_wrapper_content.setAttribute("href", "http://www.keenfootwear.com/?TAN=38-25");

						skin_wrapper_content.setAttribute("id", "skin_clicktag");
            skin_wrapper_content.setAttribute("target","_blank");						
            skin_wrapper_content.setAttribute("text","Hello");
            skin_wrapper.appendChild(skin_wrapper_content);
	 
	 var skin_wrapper_content_img = document.createElement("img");
			skin_wrapper_content_img.setAttribute("src", "http://activeint.dev.activenetwork.com/Assets/Women/makeitpink/home/blank.gif");
                        skin_wrapper_content_img.setAttribute("height", "410");	
						skin_wrapper_content_img.setAttribute("border", "0");
						skin_wrapper_content_img.setAttribute("id", "skin_clicktag_img");						
            skin_wrapper_content_img.setAttribute("text","Hello");
            skin_wrapper_content.appendChild(skin_wrapper_content_img);
	var siteDiv = document.getElementById("site");
 	var parentDiv = siteDiv.parentNode;
		 	parentDiv.insertBefore(skin_wrapper, siteDiv);
</script>


